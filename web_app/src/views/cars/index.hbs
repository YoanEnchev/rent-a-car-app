{{#> body }}
    {{#each cars}}
        <div class="mb-5">
            <p>Model: {{this.model.name}} Year: {{this.year}}</p>

            <form method="POST" action="/book-car/{{this.id}}">
                <button class="btn btn-primary">Book</button>
            </form>
        </div>
    {{/each}}
{{/body}}